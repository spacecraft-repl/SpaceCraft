{
  "name": "spacecraft",
  "version": "1.0.1",
  "description": "An open-source real-time collaborative REPL and code editor",
  "main": "server.js",
  "scripts": {
    "build": "standard --fix && webpack",
    "debug": "standard --fix && webpack && DEBUG=* node server.js",
